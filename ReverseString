import java.util.*;
import java.lang.*;
import java.io.*;
class ReverseString{
  public static void main (String[] args) throws java.lang.Exception
	{
		String s = "Java";
		String result = "";
		for(int i=s.length()-1;i>=0;i--){
		    result+= s.charAt(i);
		}
		System.out.println("Reversed String:" + result);   
		}	
}
